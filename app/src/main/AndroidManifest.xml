<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.classroomapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClassroomApp">
        <activity android:name=".classroom.ShowCurrentClassActivity"></activity>
        <activity android:name=".classroom.EditClassActivity" />
        <activity
            android:name=".classroom.AddClassroomActivity"
            android:label="Add classroom"
            android:parentActivityName=".classroom.MainActivity" />
        <activity
            android:name=".classroom.MainActivity"
            android:label="Classroom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>